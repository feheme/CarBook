@model List<ResultDetailViewModel>

<section class="ftco-section ftco-cart">
    <div class="container">
        <div class="row">
            <div class="col-md-12 ftco-animate">
                <div class="car-list">
                    <table class="table">
                        <thead class="thead-primary">
                            <tr class="text-center">
                                <th>&nbsp;</th>
                                <th>&nbsp;</th>
                                <th class="bg-primary heading">Detaylar</th>
                                
                                <th class="bg-black heading">Fiyatlar</th>
                            </tr>
                        </thead>
                        <tbody>

                                                      
                           @foreach (var item in Model)
                            {
                                
                           


                            <tr class="">
                                <td class="car-image">
                                    <div class="img" style="background-image:url(@item.Image);"></div>
                                </td>
                                <td class="product-name">
                                    <h3>@item.Name</h3>
                                   
                                </td>

                                <td class="price">                                   
                                    <div>
                       
	                                
                                        <h6>
                                            <span class="flaticon-pistons"></span>
                                            <span>@item.Transmission</span>

                                          <span class="flaticon-dashboard"></span>
                                          <span>@item.Mileage km</span>

                                        </h6>

   

                                        <h6>
                                            <span class="flaticon-car-seat"></span>         	                            	                
		                	                <span>@item.Seats</span>

                                            <span class="flaticon-diesel"></span>
                                            <span>@item.Fuel</span>                                           
                                        </h6>
                                         <span>@item.Total adet mevcuttur</span>


                                        
                                        
                                    </div>
                                </td>

                               

                                <td class="price">
                                    
                                    <div>                                       
                                         <h6>
                                            <span class="num"><small class="currency">₺</small> @item.DayPrice</span>
                                            <span class="per">/günlük</span>
                                        </h6>
                                         <h6>
                                            <span class="num"><small class="currency">₺</small> @item.MonthPrice</span>
                                            <span class="per">/aylık</span>
                                        </h6>
                                        
                                    </div>
                                </td>
                            </tr>
                              }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>